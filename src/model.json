      "SolutionOr": {
         "parameters": [
            {
              "id": "name",
              "label": "Name",
              "type": "string",
              "size": "1"
            },
            {
               "id": "or",
               "type": "integer",
               "size": 1,
               "ui": "enum",
               "default": [0],
               "domain": {
                  "Filter": 0,
                  "Plugin Writer": 1,
                  "Plugin NaN check": 2,
                  "Plugin residual": 3,
                  "Plugin sampler": 4,
                  "Plugin Time average": 5,
                  "ics": 6
               }
            },
            ["Filter",
             "PluginWriter",
             "PluginNaNcheck",
             "Pluginresidual",
             "Pluginsampler",
             "PluginTimeaverage",
             "ics"
            ]
         ],
         "children": {
            "Filter":            "SolutionOr.or[0] === 0",
            "PluginWriter":      "SolutionOr.or[0] === 1",
            "PluginNaNcheck":    "SolutionOr.or[0] === 2",
            "Pluginresidual":    "SolutionOr.or[0] === 3",
            "Pluginsampler":     "SolutionOr.or[0] === 4",
            "PluginTimeaverage": "SolutionOr.or[0] === 5",
            "ics":               "SolutionOr.or[0] === 6"
         }
      },
